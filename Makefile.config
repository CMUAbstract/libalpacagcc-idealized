ifeq ($(LIBALPACAGCC_ENABLE_DIAGNOSTICS),1)
LOCAL_CFLAGS += -DLIBALPACA_ENABLE_DIAGNOSTICS
endif

ifneq ($(LIBCOATIGCC_TIME_EV),0)
LOCAL_CFLAGS += -DLIBALPACA_TEST_EV_TIME
endif

ifneq ($(LIBCOATIGCC_WAIT_TEST),0)
LOCAL_CFLAGS += -DLIBALPACA_TEST_WAIT_TIME
endif

ifneq ($(LIBCOATIGCC_TIME_TEST),0)
LOCAL_CFLAGS += -DLIBALPACA_TEST_TIMING
endif


override CFLAGS += $(LOCAL_CFLAGS)

